    <div class="container">
        <div class="row">
            <div class="col-lg-8 posts-list">
                <div class="single-post">
                    <div class="feature-img">
                        {% block fimage %} <img class="img-fluid"  src="{{ asset('detail/assets/img/blog/single_blog_1.png') }}" alt="">{% endblock %}
